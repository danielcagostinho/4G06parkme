                android:backgroundTint="#99008000"
                android:onClick="showDialog" />

            <Button
                android:id="@+id/spot04"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/s5c534fd3a2b1de00000ed7a3"
                android:layout_marginStart="25dp"
                android:backgroundTint="#99008000"
                android:onClick="showDialog" />

            <Button
                android:id="@+id/spot05"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/spot04"
