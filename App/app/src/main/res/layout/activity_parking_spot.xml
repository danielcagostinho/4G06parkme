                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                android:layout_toEndOf="@id/s5c4946c9ac8f790000f001cf"
                android:backgroundTint="#99008000"
                android:onClick="showDialog" />

            <Button
                android:id="@+id/s5c535099a2b1de00000ed7a5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/s5c53504ea2b1de00000ed7a4"
